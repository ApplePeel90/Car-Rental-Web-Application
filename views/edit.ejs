<% include partials/header %>



<div class ="container" style="width: 30%; margin: 25px auto;">
<br>
<h1>Edit car</h1>

<form action="/cars/<%= car._id %>?_method=PUT" method="POST">

<div class="form-group">
	<label for="make">Make</label>
	<input type="text" name="make" class="form-control" value="<%= car.make %>">
</div>
<div class="form-group">
	<label for="model">Model</label>
	<input type="text" name="model" class="form-control" value="<%= car.model %>">
</div>
<div class="form-group">
	<label for="type">Type</label>
	<input type="text" name="type" class="form-control" value="<%= car.type %>">
</div>
<div class="form-group">
	<label for="Image">Image</label>
	<input type="text" name="image" class="form-control" value="<%= car.image %>">
</div>
<div class="form-group">
	<label for="Image_l">Image_l</label>
	<input type="text" name="image_l" class="form-control" value="<%= car.image %>">
</div>
<div class="form-group">
	<label for="passenger">Passenger</label>
	<input type="text" name="passenger" class="form-control" value="<%= car.passenger %>">
</div>
<div class="form-group">
	<label for="price">Price</label>
	<input type="text" name="price" class="form-control" value="<%= car.price %>">
</div>
<div class="form-group">
	<label for="location">Location</label>
	<input type="text" name="location" class="form-control" value="<%= car.location %>">
</div>
<div class="form-group">
	<label for="count">Count</label>
	<input type="text" name="count" class="form-control" value="<%= car.count %>">
</div>
<div class="form-group">
<button type="submit" class="btn btn-info">Save</buton>
</div>

</form>

<a href="/cars/1">Cancel</a>

</div>
<br><br><br>

<% include partials/footer %>